{
  "name": "portal",
  "version": "1.0.0",
  "description": "Personal portal of Aliaksandr Savin",
  "private": true,
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "husky install",
    "pre-commit": "sh .husky/pre-commit"
  },
  "dependencies": {
    "gatsby": "^5.12.4",
    "gatsby-source-filesystem": "^5.12.0",
    "gatsby-transformer-json": "^5.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "babel-preset-gatsby": "^3.12.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "typescript": "^5.1.6"
  },
  "husky": {
    "hooks": {
      "commit-msg": ".husky/commit-msg",
      "pre-commit": "sh .husky/pre-commit"
    }
  }
}
